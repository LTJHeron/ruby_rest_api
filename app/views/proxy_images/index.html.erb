<h1>Listing proxy_images</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proxy_images.each do |proxy_image| %>
  <tr>
    <td><%= proxy_image.content %></td>
    <td><%= proxy_image.user_id %></td>
    <td><%= link_to 'Show', proxy_image %></td>
    <td><%= link_to 'Edit', edit_proxy_image_path(proxy_image) %></td>
    <td><%= link_to 'Destroy', proxy_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

